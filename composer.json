{
    "name": "weatherfx/box-jwt",
    "require": {
        "guzzlehttp/guzzle": "^7.4",
        "firebase/php-jwt": "^v6.3.1",
        "laminas/laminas-config": "^3.9",
        "laminas/laminas-dependency-plugin": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Box\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "laminas/*": true
        }
    }
}
