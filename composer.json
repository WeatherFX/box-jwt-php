{
    "name": "amsxbg/box-jwt",
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "firebase/php-jwt": "^5.0",
        "laminas/laminas-config": "^3.1",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Box\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
